{
  "name": "VILauncher",
  "version": "1.0.0",
  "description": "VILauncher is a custom Minecraft launcher, developed with modern technologies that offers a smooth and user-friendly experience to start your favorite game.",
  "author": "vdkv",
  "main": "assets/js/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "dependencies": {
    "minecraft-launcher-core": "3.18.2"
  },
  "devDependencies": {
    "electron": "33.2.1",
    "electron-rebuild": "3.2.9",
    "electron-builder": "^24.0.0"
  },
  "build": {
    "appId": "com.vi.vilauncher",
    "productName": "VILauncher",
    "files": [
      "**/*",
      "assets/**/*"
    ],
    "win": {
      "target": [
        "nsis", 
        "zip"
      ],
      "icon": "assets/img/logo.png"
    }
  }
}
